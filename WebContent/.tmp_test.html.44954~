        <script type="text/javascript" src="js/extends.js">
        </script>
		<script type="text/javascript" src="js/global.js">
        </script>
        <script type="text/javascript">
            Ext.onReady(function(){
                new Ext.ffcs.LocalPanel({
                    renderTo: Ext.getBody(),
                    items: [{
                        fieldLabel: 'ddddd',
                        name: 'table_name_cn',
                        uil: '',
                        id: 'aaaaa'
                    }, new Ext.ffcs.LocalCombobox({
                        name: 'table_name',
                        id: 'aa',
                        table_name: 'table_name',
                        value: 'flow',
                        store: new Ext.data.ArrayStore({
                            fields: ['value', 'text'],
                            data: [['flow', 'flow'], ['CI', 'CI'], ['normal', 'normal']]
                        }),
                        fieldLabel: '表名'
                    }), new Ext.ffcs.LocalCombobox({
                        name: 'table_name_a1',
                        table_name: 'table_name',
                        id: 'aaa',
                        value: 'flow',
                        store: new Ext.data.ArrayStore({
                            fields: ['value', 'text'],
                            data: [['flow', 'flow'], ['CI', 'CI'], ['normal', 'normal']]
                        }),
                        fieldLabel: '表名2'
                    })],
                    title: "DDDD",
                });
                
                
                var array = Ext.query('input[name^=table_name]');
                array = Ext.DomQuery.filter(array, 'input[name^=table_name_cn]', true);//过滤掉
                Ext.each(array, function(item, index){
                    Ext.getCmp(item.id).getStore().loadData(table_names);
                    Ext.getCmp(item.id).setValue(table_names == null ? "" : table_names[0][0]);
                });
            });
        </script>
