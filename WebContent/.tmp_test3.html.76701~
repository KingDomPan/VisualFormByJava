        <script>
            Ext.onReady(function(){
				var editor=new Ext.ux.grid.RowEditor({
					saveText:'更新',
					cancelText:'取消'
				});
				var User=Ext.data.Record.create([{name:'username'},{name:'password'}]);
				users=[];
				for(var i=0;i<20;i++){
					var user=new User({
						username:'1111',
						password:'2222'
					});
					users.push(user);
				}
                var grid = new Ext.ffcs.EasyGrid({
                    data: users,
                    Record: User,
					renderTo:'win'
                });
            });
        </script>
